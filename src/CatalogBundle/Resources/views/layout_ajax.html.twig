{% extends '::base.html.twig' %}

{% set dir = 'catalog' %}

{% block stylesheets %}
    <link href='{{ asset("/bundles/#{dir}/css/font-awesome.min.css") }}' rel="stylesheet" />
    {#<link href='{{ asset("/bundles/#{dir}/css/all.css") }}' rel="stylesheet" />#}
    <link href='{{ asset("/bundles/#{dir}/css/all.min.css") }}' rel="stylesheet" />
{% endblock %}

{% block title %}Grapho :: {{ title }}{% endblock %}

{% block body %}
    <style>@charset "UTF-8";.main,.wrapper,header{position:relative}a,abbr,acronym,address,applet,article,aside,audio,b,big,blockquote,body,canvas,caption,center,cite,code,dd,del,details,dfn,div,dl,dt,em,embed,fieldset,figcaption,figure,footer,form,h1,h2,h3,h4,h5,h6,header,hgroup,html,i,iframe,img,ins,kbd,label,legend,li,mark,menu,nav,object,ol,output,p,pre,q,ruby,s,samp,section,small,span,strike,strong,sub,summary,sup,table,tbody,td,tfoot,th,thead,time,tr,tt,u,ul,var,video{margin:0;padding:0;border:0;font:inherit;font-size:100%;vertical-align:baseline}header,nav{font-family:"Rosbank Regular",sans-serif}html{line-height:1}ol,ul{list-style:none}table{border-collapse:collapse;border-spacing:0}caption,td,th{text-align:left;font-weight:400;vertical-align:middle}.hlebon-content strong,.left-sidebar i{font-weight:700}blockquote,q{quotes:none}blockquote:after,blockquote:before,q:after,q:before{content:"";content:none}a img{border:none}article,aside,details,figcaption,figure,footer,header,hgroup,main,menu,nav,section,summary{display:block}@font-face{font-family:'Rosbank Light';src:url(/bundles/catalog/fonts/rosbank/RBSTTLIG.TTF) format("truetype");font-weight:100;font-style:normal}@font-face{font-family:'Rosbank Regular';src:url(/bundles/catalog/fonts/rosbank/RBSTTREG.TTF) format("truetype");font-weight:100;font-style:normal}*{box-sizing:border-box}.wrapper{margin:0 auto;max-width:968px}body{font-size:1rem;background-image:url(data:image/svg+xml;base64,);background-size:100%;background-image:-webkit-gradient(linear,0 50%,100% 50%,color-stop(0,#262d36),color-stop(50%,#3c5b80),color-stop(100%,#262d36));background-image:-moz-linear-gradient(left,#262d36 0,#3c5b80 50%,#262d36 100%);background-image:-webkit-linear-gradient(left,#262d36 0,#3c5b80 50%,#262d36 100%);background-image:linear-gradient(to right,#262d36 0,#3c5b80 50%,#262d36 100%)}.clearfix:after,.clearfix:before{content:" ";display:table}.clearfix:after{clear:both}header{z-index:4;color:#fff;height:10vh;display:flex;align-items:center;font-size:1.17rem}header .logo{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;width:25%;float:left;padding-left:1.5%;padding-right:1.5%}header .contacts,header .contacts .phones{-moz-box-sizing:border-box;-webkit-box-sizing:border-box}header .logo a img{margin-left:15px;width:190px}header .contacts,header .contacts .address,header .contacts .phones{width:50%;float:left;padding-left:1.5%}header .contacts{box-sizing:border-box;margin-left:25%;padding-right:0;line-height:1.7rem}header .contacts .phones{text-align:center;position:relative;box-sizing:border-box;padding-right:1.5%}header .contacts .phones i{font-size:2rem;position:absolute;left:20px;top:11px}header .contacts .address{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;padding-right:0}nav{font-size:1.2rem}@media only screen and (min-width:1001px){.menu-top__button,nav .logo{display:none}.menu-top{height:5vh;background:#3D526D;box-shadow:0 1px 14px #3D526D}.menu-top>ul{list-style:none;padding:0;margin:0;display:flex;justify-content:space-between;align-items:center;height:100%;position:relative}.menu-top>ul li{-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;width:25%;float:left;text-align:center;height:100%;display:table;transition:background-color .3s ease,color .3s ease}.menu-top>ul li:hover{background:#49688B}.menu-top>ul li a{text-decoration:none;padding:20px;display:table-cell;vertical-align:middle;color:#fff}</style>
    {% block templates %}{% endblock %}

    <div class="wrapper">
        {% block header %}
            {{ include('CatalogBundle:Ajax:header.ajax.twig') }}
        {% endblock %}

        {% block nav %}
            <div id="nav-region"></div>
        {% endblock %}

        <div class="main">
            <div id="sidebar-region"></div>
            <div id="content-region"></div>
        </div>

        <footer>
            <div class="wrapper-boarder wrapper-header">
                +395 XX XXX-XX-XX
            </div>
        </footer>
    </div>
{% endblock %}

{% block javascripts %}{% endblock %}